@using ABS_LMS.Models.Security
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title -AnderosnBPO</title>
    @Styles.Render("~/bundles/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/bundles/base")
    <meta name="description" content="Anderson Business Solutions Pvt. Ltd." />
</head>
<body>

    <div class="header container-fluid">
        <div class="row">
            @Html.Partial("~/Views/Shared/_HeaderPartial.cshtml")
        </div>
    </div>
    <div class="container">
        @RenderBody()
    </div>
    <div class="clear"></div>
    <div class="footer container">
        <p>
            <span>&copy; @DateTime.Today.Year AndersonBPO</span>
            <span class="Telephone">Tel: 12345678</span>
            <span>Email: <a href="mailto:andersonbpo.com">andersonbpo.com</a></span>
            @*<span>@Html.ActionLink("Privacy Policy", "PrivacyPolicy", "Home")</span>*@
        </p>
    </div>
     @Scripts.Render("~/bundles/jquerylib")
    <script src="~/Scripts/bootstrap.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
